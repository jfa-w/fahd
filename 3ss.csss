@font-face{font-family:jazeera;src:url(https://www.fontstatic.com/fonts/jazeera/jazeera.eot?#iefix);src:local(الجزيرة),local(jazeera),url(https://www.fontstatic.com/fonts/jazeera/jazeera.woff) format("woff")}
@font-face{font-family:jazeera-light;src:url(https://www.fontstatic.com/fonts/jazeera-light/jazeera-light.eot?#iefix);src:local('الجزيرة خفيف'),local(jazeera-light),url(https://www.fontstatic.com/fonts/jazeera-light/jazeera-light.woff) format("woff")}
#as{font-family:jazeera-light,FontAwesome;margin-bottom:2px;color:#fffffa;border-radius:5px 5px 5px 5px;text-shadow:2px 0 8px rgba(255,255,219),0 2px 10px #fff!important;border:1px solid #ffe0f6;height:33px;text-align:center}
a.btn.minix.fa.fa-comment.fr{display:none}
ul.nav.nav-tabs.fl li{background:#fff;width:33%;border:1px solid#a7a4a4;margin-left:1px;text-align:center;border-radius:15px 15px 7px 7px}
.well{font-family:jazeera,FontAwesome;font-size:16px!important;padding:3px!important;text-align:center!important;width:100%;color:#000;white-space:nowrap;border-radius:15px 15px 15px 15px}
.nav-tabs>li>a{border-radius:15px 15px 7px 7px;line-height:9px;padding:10px 11px!important}
ul.nav.nav-tabs.fl li a{text-align:center}
.btn-ali{background-image:-webkit-gradient(linear,left top,left bottom,from(#7a9fb1),to(#75838a));border-radius:15px 15px 7px 7px;border:1px solid #9db0b9;font-family:jazeera,FontAwesome;padding:4px 3px 3px 4px}
.label-danger{background-color: #f8f8ff;color: #000000;}
